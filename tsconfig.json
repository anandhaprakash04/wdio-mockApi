{
  "compilerOptions": {
    "target": "ES6",
    "outDir": "./.tsbuild/",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": false,
    "noImplicitAny": false,
    "baseUrl": ".",
    "resolveJsonModule": true,
    "paths": {
      "@pageObject/*": ["/main/page-objects/*"],
      "@fixture/*": ["/main/fixture/*"]
    },
    "types": ["node", "@wdio/sync", "@wdio/jasmine-framework", "expect-webdriverio"]
  },
  "include": [
    "./main/**/*.ts"
  ]
}
